<div id="content">
			<div id="content-header">
				<h1>User Profiles Gallery</h1>
				
			</div>
			<div id="breadcrumb">
				<%= link_to admin_admin_index_url, {:title => "Go to Home", :class => "tip-bottom"} do %>
				<i class="icon-home"></i> Home
				<% end %>
				<a href="#" class="current">User Images</a>
			</div>
			<div class="container-fluid">
				<div class="row-fluid">
					<div class="span12">
						<div class="widget-box">
							<div class="widget-title">
								<span class="icon">
									<i class="icon-picture"></i>
								</span>
								<h5>Gallery</h5>
								<span class="label label-info"><%= @count %></span>
							</div>
							<div class="widget-content">
								<ul class="thumbnails">
									<% @photos.each do |photo| %>
									<li class="span2">
										<%= link_to user_path(photo.imageable_id), :class => "thumbnail" do %>
											<%= image_tag(photo.image.url(:medium), :size => '140x140') %>
										<% end %>
										<div class="actions">
											<%= link_to edit_user_path(photo.imageable_id) do %><i class="icon-pencil icon-white"></i><% end %>
											<a href="#"><i class="icon-remove icon-white"></i></a>
										</div>
									</li>
									<% end %>
								</ul>
							</div>
						</div>
					</div>
				</div>

					<div class="widget-box">
						<div class="widget-title">
							<span class="icon">
								<i class="icon-exclamation-sign"></i>
							</span>
							<h5>Navigation</h5>
						</div>
						<div class="widget-content">
							<div class="pagination">
							<%= paginate @photos %>
							</div>
					</div>
				</div>