<div class="span12">
  <div class="well well-small">
    <h3>
      Physical Description
    </h3>
    <table class="table table-striped table-hover">								
		  <tbody>
				<tr>
				<td><b>Age:</b> <%= @user.characteristics.age %></td>
	      <td><b>Height:</b> <%= @user.characteristics.height %></td>
				<td><b>Weight:</b> <%= @user.characteristics.weight %></td>    
				<td><b>Ethnicity:</b> <%= @user.characteristics.ethnicity %></td>
				</tr>
				<tr>
		      <td><b>Body Type:</b> <%= @user.characteristics.bodytype %></td>
		      <td><b>Skin Colour:</b> <%= @user.characteristics.skin_color %></td>
					<td><b>Eye Colour:</b> <%= @user.characteristics.eye_color %></td>
		      <td><b>Hair Colour:</b> <%= @user.characteristics.hair_color %></td>
		    </tr>
				<tr>
		      <td><b>Chest:</b> <%= @user.characteristics.chest %></td>
					<td><b>Waist:</b> <%= @user.characteristics.waist %></td>
		      <td><b>Hips:</b> <%= @user.characteristics.hips %></td>
		      <td><b>Dress:</b> <%= @user.characteristics.dress_size %></td>
		    </tr>
		  </tbody>
		</table>
  </div>
</div>